{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "baseUrl": "src",
      "rootDir": "src"
   },
   "include": [
      "src/models/*.ts",
      "src/migrations/*ts",
      "src/subscribers/*ts"
   ],
   "exclude": [
      "node_modules"
    ]
}