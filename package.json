{
    "name": "next-app",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">= 14.5.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "typeorm": "cross-env TS_NODE_PROJECT=tsconfig.typeorm.json TS_NODE_TRANSPILE_ONLY=true ts-node ./node_modules/typeorm/cli.js",
        "test": "jest --config=config/jest/jest.config.js",
        "test:watch": "jest --config=config/jest/jest.config.js --watch",
        "test:coverage": "jest --config=config/jest/jest.config.js --coverage",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && npm run test"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint '*/**/*.{js,ts,tsx}' --fix"
        ]
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.4.0",
        "class-validator": "^0.12.2",
        "next": "9.5.0",
        "next-auth": "nextauthjs/next-auth#main",
        "pg": "^8.3.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-redux": "^7.2.1",
        "typeorm": "^0.2.25"
    },
    "devDependencies": {
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^26.0.7",
        "@types/node": "^14.0.26",
        "@types/react": "^16.9.43",
        "@types/react-redux": "^7.1.9",
        "@types/webpack-env": "^1.15.2",
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "babel-jest": "^26.1.0",
        "cross-env": "^7.0.2",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.5.0",
        "eslint": "^7.5.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.5",
        "husky": "^4.2.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.1.0",
        "jest-fetch-mock": "^3.0.3",
        "lint-staged": "^10.2.11",
        "prettier": "^2.0.5",
        "ts-jest": "^26.1.3",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    }
}
